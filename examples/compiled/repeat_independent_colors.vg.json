{
    "$schema": "https://vega.github.io/schema/vega/v4.json",
    "autosize": "pad",
    "padding": 5,
    "width": 200,
    "height": 200,
    "data": [
        {
            "name": "source_0",
            "url": "data/cars.json",
            "format": {
                "type": "json"
            }
        },
        {
            "name": "data_0",
            "source": "source_0",
            "transform": [
                {
                    "type": "formula",
                    "expr": "toNumber(datum[\"Horsepower\"])",
                    "as": "Horsepower"
                },
                {
                    "type": "formula",
                    "expr": "toNumber(datum[\"Miles_per_Gallon\"])",
                    "as": "Miles_per_Gallon"
                },
                {
                    "type": "filter",
                    "expr": "datum[\"Horsepower\"] !== null && !isNaN(datum[\"Horsepower\"]) && datum[\"Miles_per_Gallon\"] !== null && !isNaN(datum[\"Miles_per_Gallon\"])"
                }
            ]
        },
        {
            "name": "data_1",
            "source": "source_0",
            "transform": [
                {
                    "type": "formula",
                    "expr": "toNumber(datum[\"Horsepower\"])",
                    "as": "Horsepower"
                },
                {
                    "type": "formula",
                    "expr": "toNumber(datum[\"Miles_per_Gallon\"])",
                    "as": "Miles_per_Gallon"
                },
                {
                    "type": "filter",
                    "expr": "datum[\"Horsepower\"] !== null && !isNaN(datum[\"Horsepower\"]) && datum[\"Miles_per_Gallon\"] !== null && !isNaN(datum[\"Miles_per_Gallon\"])"
                }
            ]
        }
    ],
    "layout": {
        "padding": {
            "row": 10,
            "column": 10
        },
        "columns": 2,
        "bounds": "full",
        "align": "all"
    },
    "marks": [
        {
            "type": "group",
            "name": "child_Origin_group",
            "style": "cell",
            "encode": {
                "update": {
                    "width": {
                        "signal": "width"
                    },
                    "height": {
                        "signal": "height"
                    }
                }
            },
            "marks": [
                {
                    "name": "child_Origin_marks",
                    "type": "symbol",
                    "style": [
                        "point"
                    ],
                    "from": {
                        "data": "data_0"
                    },
                    "encode": {
                        "update": {
                            "opacity": {
                                "value": 0.7
                            },
                            "fill": {
                                "value": "transparent"
                            },
                            "stroke": {
                                "scale": "child_Origin_color",
                                "field": "Origin"
                            },
                            "x": {
                                "scale": "child_Origin_x",
                                "field": "Horsepower"
                            },
                            "y": {
                                "scale": "child_Origin_y",
                                "field": "Miles_per_Gallon"
                            }
                        }
                    }
                }
            ],
            "axes": [
                {
                    "scale": "child_Origin_x",
                    "orient": "bottom",
                    "grid": false,
                    "title": "Horsepower",
                    "labelFlush": true,
                    "labelOverlap": true,
                    "tickCount": {
                        "signal": "ceil(width/40)"
                    },
                    "zindex": 1
                },
                {
                    "scale": "child_Origin_x",
                    "orient": "bottom",
                    "grid": true,
                    "tickCount": {
                        "signal": "ceil(width/40)"
                    },
                    "gridScale": "child_Origin_y",
                    "domain": false,
                    "labels": false,
                    "maxExtent": 0,
                    "minExtent": 0,
                    "ticks": false,
                    "zindex": 0
                },
                {
                    "scale": "child_Origin_y",
                    "orient": "left",
                    "grid": false,
                    "title": "Miles_per_Gallon",
                    "labelOverlap": true,
                    "tickCount": {
                        "signal": "ceil(height/40)"
                    },
                    "zindex": 1
                },
                {
                    "scale": "child_Origin_y",
                    "orient": "left",
                    "grid": true,
                    "tickCount": {
                        "signal": "ceil(height/40)"
                    },
                    "gridScale": "child_Origin_x",
                    "domain": false,
                    "labels": false,
                    "maxExtent": 0,
                    "minExtent": 0,
                    "ticks": false,
                    "zindex": 0
                }
            ],
            "legends": [
                {
                    "stroke": "child_Origin_color",
                    "title": "Origin",
                    "encode": {
                        "symbols": {
                            "update": {
                                "fill": {
                                    "value": "transparent"
                                },
                                "opacity": {
                                    "value": 0.7
                                }
                            }
                        }
                    }
                }
            ]
        },
        {
            "type": "group",
            "name": "child_Cylinders_group",
            "style": "cell",
            "encode": {
                "update": {
                    "width": {
                        "signal": "width"
                    },
                    "height": {
                        "signal": "height"
                    }
                }
            },
            "marks": [
                {
                    "name": "child_Cylinders_marks",
                    "type": "symbol",
                    "style": [
                        "point"
                    ],
                    "from": {
                        "data": "data_1"
                    },
                    "encode": {
                        "update": {
                            "opacity": {
                                "value": 0.7
                            },
                            "fill": {
                                "value": "transparent"
                            },
                            "stroke": {
                                "scale": "child_Cylinders_color",
                                "field": "Cylinders"
                            },
                            "x": {
                                "scale": "child_Cylinders_x",
                                "field": "Horsepower"
                            },
                            "y": {
                                "scale": "child_Cylinders_y",
                                "field": "Miles_per_Gallon"
                            }
                        }
                    }
                }
            ],
            "axes": [
                {
                    "scale": "child_Cylinders_x",
                    "orient": "bottom",
                    "grid": false,
                    "title": "Horsepower",
                    "labelFlush": true,
                    "labelOverlap": true,
                    "tickCount": {
                        "signal": "ceil(width/40)"
                    },
                    "zindex": 1
                },
                {
                    "scale": "child_Cylinders_x",
                    "orient": "bottom",
                    "grid": true,
                    "tickCount": {
                        "signal": "ceil(width/40)"
                    },
                    "gridScale": "child_Cylinders_y",
                    "domain": false,
                    "labels": false,
                    "maxExtent": 0,
                    "minExtent": 0,
                    "ticks": false,
                    "zindex": 0
                },
                {
                    "scale": "child_Cylinders_y",
                    "orient": "left",
                    "grid": false,
                    "title": "Miles_per_Gallon",
                    "labelOverlap": true,
                    "tickCount": {
                        "signal": "ceil(height/40)"
                    },
                    "zindex": 1
                },
                {
                    "scale": "child_Cylinders_y",
                    "orient": "left",
                    "grid": true,
                    "tickCount": {
                        "signal": "ceil(height/40)"
                    },
                    "gridScale": "child_Cylinders_x",
                    "domain": false,
                    "labels": false,
                    "maxExtent": 0,
                    "minExtent": 0,
                    "ticks": false,
                    "zindex": 0
                }
            ],
            "legends": [
                {
                    "stroke": "child_Cylinders_color",
                    "title": "Cylinders",
                    "encode": {
                        "symbols": {
                            "update": {
                                "fill": {
                                    "value": "transparent"
                                },
                                "opacity": {
                                    "value": 0.7
                                }
                            }
                        }
                    }
                }
            ]
        }
    ],
    "scales": [
        {
            "name": "child_Origin_x",
            "type": "linear",
            "domain": {
                "data": "data_0",
                "field": "Horsepower"
            },
            "range": [
                0,
                {
                    "signal": "width"
                }
            ],
            "nice": true,
            "zero": true
        },
        {
            "name": "child_Origin_y",
            "type": "linear",
            "domain": {
                "data": "data_0",
                "field": "Miles_per_Gallon"
            },
            "range": [
                {
                    "signal": "height"
                },
                0
            ],
            "nice": true,
            "zero": true
        },
        {
            "name": "child_Origin_color",
            "type": "ordinal",
            "domain": {
                "data": "data_0",
                "field": "Origin",
                "sort": true
            },
            "range": "category"
        },
        {
            "name": "child_Cylinders_x",
            "type": "linear",
            "domain": {
                "data": "data_1",
                "field": "Horsepower"
            },
            "range": [
                0,
                {
                    "signal": "width"
                }
            ],
            "nice": true,
            "zero": true
        },
        {
            "name": "child_Cylinders_y",
            "type": "linear",
            "domain": {
                "data": "data_1",
                "field": "Miles_per_Gallon"
            },
            "range": [
                {
                    "signal": "height"
                },
                0
            ],
            "nice": true,
            "zero": true
        },
        {
            "name": "child_Cylinders_color",
            "type": "ordinal",
            "domain": {
                "data": "data_1",
                "field": "Cylinders",
                "sort": true
            },
            "range": "category"
        }
    ],
    "config": {
        "axisY": {
            "minExtent": 30
        }
    }
}
